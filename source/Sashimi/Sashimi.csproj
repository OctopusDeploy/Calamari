<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Sashimi.AzureServiceFabric</AssemblyName>
    <RootNamespace>Sashimi.AzureServiceFabric</RootNamespace>
    <TargetFramework>netstandard2.1</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <OutputPath>bin\</OutputPath>
    <Nullable>enable</Nullable>
    <PackageProjectUrl>https://github.com/OctopusDeploy/Sashimi.AzureServiceFabric</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>NU5119</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="**\*.ps1" />
    <EmbeddedResource Include="**\*.sh" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\artifacts\Calamari.AzureServiceFabric.zip">
      <Visible>false</Visible>
      <LinkBase>tools</LinkBase>
      <Pack>true</Pack>
      <PackagePath>tools/</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Sashimi.AzureScripting" Version="14.0.0" />
    <PackageReference Include="Sashimi.Server.Contracts" Version="14.0.1" />
    <PackageReference Include="Sashimi.AzureScripting" Version="13.2.0" />
    <PackageReference Include="Sashimi.Server.Contracts" Version="13.3.1-enh-create-deplo0010" />
  </ItemGroup>
</Project>
