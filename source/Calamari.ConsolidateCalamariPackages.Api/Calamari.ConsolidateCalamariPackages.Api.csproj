<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <RootNamespace>Octopus.Calamari.ConsolidatedPackage.Api</RootNamespace>
        <AssemblyName>Octopus.Calamari.ConsolidatedPackage.Api</AssemblyName>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>default</LangVersion>
        <Authors>Octopus Deploy</Authors>
        <Copyright>Octopus Deploy Pty Ltd</Copyright>
        <Description>
            Octopus Deploy is an automated release management tool for modern developers and DevOps teams.

            This package contains the API for a compression process which flattens and expands multiple zips
            into a single archive.
        </Description>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    </PropertyGroup>

</Project>
